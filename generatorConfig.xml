<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<classPathEntry
		location="D:/Java/maven/repos/mysql/mysql-connector-java/5.1.21/mysql-connector-java-5.1.21.jar" />
	<!-- local class jar -->
	<context id="DB2Tables" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://192.168.247.1:3306/resignwall" userId="root"
			password="root" />
		<!-- data -->
		<javaModelGenerator targetPackage="com.fatface.data"
			targetProject="resignwall" />
		<!-- mappper -->
		<sqlMapGenerator targetPackage="com.fatface.dao"
			targetProject="resignwall" />
		<!-- dto -->
		<javaClientGenerator targetPackage="com.fatface.dao"
			targetProject="resignwall" type="XMLMAPPER">
			<property name="exampleMethodVisibility" value="private" />
			<property name="rootInterface" value="com.fatface.dao.SqlMapper" />
		</javaClientGenerator>

		<table tableName="user" enableSelectByExample="false"
			enableDeleteByExample="false" enableCountByExample="false"
			enableUpdateByExample="false">
			<generatedKey column="id" sqlStatement="MySql" identity="true"
				type="post" />
		</table>
		<table tableName="employee" enableSelectByExample="false"
			enableDeleteByExample="false" enableCountByExample="false"
			enableUpdateByExample="false">
			<generatedKey column="id" sqlStatement="MySql" identity="true"
				type="post" />
		</table>
	</context>
</generatorConfiguration>
